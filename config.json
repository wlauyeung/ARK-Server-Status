{
  "prefix": "!mss",
  "queryInterval": 10,
  "offlineCounterThreshold": 3,
  "defaultMute": false,
  "commands": {
    "setchannel": {
      "admin": true,
      "desc": "Set the notification channel",
      "alias": [
        "setchannel"
      ],
      "usage": [
        "<channel_id>"
      ]
    },
    "track": {
      "admin": true,
      "desc": "Add a server to the tracking list",
      "alias": [
        "track"
      ],
      "usage": [
        "<server_name>"
      ]
    },
    "untrack": {
      "admin": true,
      "desc": "Remove a server from the tracking list",
      "alias": [
        "untrack"
      ],
      "usage": [
        "<server_name>"
      ]
    },
    "stalk": {
      "admin": false,
      "desc": "Look for a player on a server",
      "alias": [
        "stalk"
      ],
      "usage": [
        "<player_name>",
        "<server_name> <player_name>"
      ]
    },
    "status": {
      "admin": false,
      "desc": "Check the status of a server",
      "alias": [
        "status"
      ],
      "usage": [
        "<server_name>"
      ]
    },
    "listplayers": {
      "admin": false,
      "desc": "List all players on a server",
      "alias": [
        "listplayers",
        "lp"
      ],
      "usage": [
        "<server_name>"
      ]
    },
    "mute": {
      "admin": true,
      "desc": "Mute all or a server",
      "alias": [
        "mute"
      ],
      "usage": [
        "<server_name/all>"
      ]
    },
    "unmute": {
      "admin": true,
      "desc": "Unmute all or a server",
      "alias": [
        "unmute"
      ],
      "usage": [
        "<server_name/all>"
      ]
    },
    "help": {
      "admin": false,
      "desc": "Display all available commands with usages",
      "alias": [
        "help"
      ],
      "usage": []
    }
  }
}