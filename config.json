{
  "prefix": "!ss",
  "serverListUpdateInterval": 1,
  "queryInterval": 10,
  "offlineCounterThreshold": 3,
  "defaultMute": false,
  "gsCooldown": 60, 
  "gsTimeout": 10,
  "commands": {
    "setchannel": {
      "admin": true,
      "desc": "Set the notification channel",
      "alias": [
        "setchannel"
      ],
      "usage": [
        "channel_id"
      ]
    },
    "track": {
      "admin": true,
      "desc": "Add a server to the tracking list",
      "alias": [
        "track"
      ],
      "usage": [
        "server_name"
      ]
    },
    "untrack": {
      "admin": true,
      "desc": "Remove a server from the tracking list",
      "alias": [
        "untrack"
      ],
      "usage": [
        "server_name"
      ]
    },
    "stalk": {
      "admin": false,
      "desc": "Look for a player on a server",
      "alias": [
        "stalk"
      ],
      "usage": [
        "player_name",
        "server_name player_name"
      ]
    },
    "status": {
      "admin": false,
      "desc": "Check the status of a server",
      "alias": [
        "status"
      ],
      "usage": [
        "server_name"
      ]
    },
    "listplayers": {
      "admin": false,
      "desc": "List all players on a server",
      "alias": [
        "listplayers",
        "lp"
      ],
      "usage": [
        "server_name"
      ]
    },
    "rates": {
      "admin": false,
      "desc": "Check the current rates on official servers",
      "alias": [
        "rates"
      ],
      "usage": []
    },
    "mute": {
      "admin": true,
      "desc": "Mute all or a server",
      "alias": [
        "mute"
      ],
      "usage": [
        "server_name"
      ]
    },
    "unmute": {
      "admin": true,
      "desc": "Unmute all or a server",
      "alias": [
        "unmute"
      ],
      "usage": [
        "server_name"
      ]
    },
    "help": {
      "admin": false,
      "desc": "Display all available commands with usages",
      "alias": [
        "help"
      ],
      "usage": []
    },
    "uptime": {
      "admin": false,
      "desc": "Check the uptime percentage of a server",
      "alias": [
        "uptime",
        "ut"
      ],
      "usage": [
        "server_name"
      ]
    },
    "globalstalk": {
      "admin": false,
      "desc": "Stalk a player from all available servers",
      "alias": [
        "globalstalk",
        "gs"
      ],
      "usage": [
        "player_name"
      ]
    },
    "playercount": {
      "admin": false,
      "desc": "Gives the playercount of the entire network",
      "alias": [
        "playercount",
        "pc"
      ],
      "usage": []
    }
  }
}